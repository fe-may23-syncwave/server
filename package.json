{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ignore-path .gitignore",
    "fix-style": "npm run format && npm run lint -- --fix",
    "make-model": "sequelize-cli model:generate",
    "make-db-seed": "sequelize-cli seed:generate --name",
    "db-seed": "sequelize-cli db:seed:all",
    "db-migrate": "sequelize-cli db:migrate",
    "migrate-undo-all": "sequelize-cli db:migrate:undo:all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fe-may23-syncwave/server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fe-may23-syncwave/server/issues"
  },
  "homepage": "https://github.com/fe-may23-syncwave/server#readme",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript": "^2.1.5",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1"
  }
}
